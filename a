-- Functions
local function validateKey(key)
    for _, validKey in pairs(VALID_KEYS) do
        if key == validKey then
            return true
        end
    end
    return false
end

local function executeScript()
    StatusLabel.Text = "‚úÖ Key valid! Loading script..."
    StatusLabel.TextColor3 = Color3.fromRGB(0, 255, 0)
    
    repeat wait() until game:IsLoaded() and game.Players.LocalPlayer
    
    local success, error = pcall(function()
        loadstring(game:HttpGet("https://pastefy.app/gPOwlw5K/raw"))()
    end)
    
    if success then
        StatusLabel.Text = "‚úÖ Script loaded successfully!"
        wait(2)
        ScreenGui:Destroy()
    else
        StatusLabel.Text = "‚ùå Error loading script"
        StatusLabel.TextColor3 = Color3.fromRGB(255, 100, 100)
    end
end

local function copyToClipboard(text)
    if setclipboard then
        setclipboard(text)
        CopyButton.Text = "COPIED!"
        wait(1)
        CopyButton.Text = "COPY"
    end
end

-- Button Events
SubmitButton.MouseButton1Click:Connect(function()
    local key = KeyInput.Text
    if key == "" then
        StatusLabel.Text = "‚ùå Please enter a key!"
        StatusLabel.TextColor3 = Color3.fromRGB(255, 100, 100)
        return
    end
    
    if validateKey(key) then
        executeScript()
    else
        StatusLabel.Text = "‚ùå Invalid key! Please try again."
        StatusLabel.TextColor3 = Color3.fromRGB(255, 100, 100)
    end
end)

BuyPremiumButton.MouseButton1Click:Connect(function()
    PremiumUI.Visible = true
end)

CopyButton.MouseButton1Click:Connect(function()
    copyToClipboard("082281712356")
end)

CloseButton.MouseButton1Click:Connect(function()
    PremiumUI.Visible = false
end)

-- Enter key to submit
KeyInput.FocusLost:Connect(function(enterPressed)
    if enterPressed then
        SubmitButton.MouseButton1Click:Fire()
    end
end)

print("üîë Key Gateway Script Loaded!")

